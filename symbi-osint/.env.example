# Environment variables for Symbi-OSINT Docker setup
# Copy this file to .env and configure your values

# Database Configuration
POSTGRES_DB=symbi_osint
POSTGRES_USER=symbi
POSTGRES_PASSWORD=symbi_password

# Redis Configuration  
REDIS_PASSWORD=symbi_redis_password

# Symbiont Configuration
RUST_LOG=info
SYMBI_CONFIG_PATH=/etc/symbi
SYMBI_AGENTS_PATH=/var/lib/symbi/agents

# API Keys (configure as needed)
OPENAI_API_KEY=your_openai_key_here
VIRUSTOTAL_API_KEY=your_virustotal_key_here
ABUSEIPDB_API_KEY=your_abuseipdb_key_here
SHODAN_API_KEY=your_shodan_key_here

# Grafana Configuration
GF_SECURITY_ADMIN_PASSWORD=admin

# Network Configuration
COMPOSE_PROJECT_NAME=symbi-osint